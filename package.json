{
	"name": "typerestjs",
	"version": "2.3.2",
	"description": "A Type-safe RESTful server framework made with Zod & Fastify",
	"license": "MIT",
	"type": "module",
	"types": "lib/index.ts",
	"main": "lib/index.js",
	"exports": {
		".": {
			"types": "./lib/index.ts",
			"default": "./lib/index.js"
		},
		"./telemetry": {
			"types": "./lib/telemetry.ts",
			"default": "./lib/telemetry.js"
		}
	},
	"files": [
		"lib"
	],
	"homepage": "https://github.com/jonasroussel/typerestjs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonasroussel/typerestjs.git"
	},
	"bugs": {
		"url": "https://github.com/jonasroussel/typerestjs/issues"
	},
	"author": {
		"name": "Jonas Roussel",
		"email": "go.jroussel@gmail.com",
		"url": "https://github.com/jonasroussel"
	},
	"keywords": [
		"type",
		"safe",
		"restful",
		"server",
		"framework",
		"zod",
		"fastify"
	],
	"scripts": {
		"build": "npm run clean && cp -r ./src/* ./lib && tsc",
		"clean": "rimraf lib/*",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@fastify/cookie": "^9.2.0",
		"@fastify/cors": "^8.4.1",
		"@fastify/formbody": "^7.4.0",
		"@fastify/multipart": "^8.0.0",
		"@fastify/rate-limit": "^9.0.0",
		"@fastify/static": "^6.12.0",
		"@opentelemetry/api": "^1.7.0",
		"@opentelemetry/exporter-jaeger": "^1.18.1",
		"@opentelemetry/exporter-trace-otlp-grpc": "^0.45.1",
		"@opentelemetry/resources": "^1.18.1",
		"@opentelemetry/sdk-metrics": "^1.18.1",
		"@opentelemetry/sdk-node": "^0.45.1",
		"@opentelemetry/sdk-trace-node": "^1.18.1",
		"@opentelemetry/semantic-conventions": "^1.18.1",
		"fastify": "^4.24.3",
		"glob": "^10.3.10",
		"mime-types": "^2.1.35",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mime-types": "^2.1.4",
		"@types/node": "^20.10.0",
		"prettier": "^3.1.0",
		"rimraf": "^5.0.5",
		"typescript": "^5.3.2"
	}
}
